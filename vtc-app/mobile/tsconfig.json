{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "strict": true,
    "noEmit": true,
    "types": ["node", "react", "react-native"],
    "skipLibCheck": true
  },
  "include": ["App.tsx", "src/**/*.ts", "src/**/*.tsx"]
}
